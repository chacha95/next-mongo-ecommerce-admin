{
  "extends": ["prettier", "next", "plugin:react/recommended"],
  "plugins": ["unicorn"],
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "@next/next/no-document-import-in-page": "off",
    "prefer-const": "error",
    "react-hooks/exhaustive-deps": "error"
  }
}
